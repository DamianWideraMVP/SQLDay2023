{
	"name": "02inc load up gdelt",
	"properties": {
		"activities": [
			{
				"name": "ListAllFolders",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"dataset": {
						"referenceName": "src_filelist",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "TraverseFolders",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "ListAllFolders",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('ListAllFolders').output.value",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "ExecCopyFolder",
							"type": "ExecutePipeline",
							"dependsOn": [
								{
									"activity": "IfAnyFiles",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "12 copy history to bronze no delete",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"UYear": {
										"value": "@item().Byear",
										"type": "Expression"
									},
									"UMonth": {
										"value": "@item().Bmonth",
										"type": "Expression"
									}
								}
							}
						},
						{
							"name": "IfAnyFiles",
							"type": "IfCondition",
							"dependsOn": [
								{
									"activity": "ListFiles",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"expression": {
									"value": "@greater(length(activity('ListFiles').output.childItems), 0)",
									"type": "Expression"
								},
								"ifFalseActivities": [
									{
										"name": "FailNoFiles",
										"type": "Fail",
										"dependsOn": [],
										"userProperties": [],
										"typeProperties": {
											"message": "No files in the source folder",
											"errorCode": "5001"
										}
									}
								],
								"ifTrueActivities": [
									{
										"name": "WaitTrue",
										"type": "Wait",
										"dependsOn": [],
										"userProperties": [],
										"typeProperties": {
											"waitTimeInSeconds": 1
										}
									}
								]
							}
						},
						{
							"name": "ListFiles",
							"type": "GetMetadata",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataset": {
									"referenceName": "src_dir_file",
									"type": "DatasetReference",
									"parameters": {
										"DYear": {
											"value": "@item().Byear",
											"type": "Expression"
										},
										"DMonth": {
											"value": "@item().Bmonth",
											"type": "Expression"
										}
									}
								},
								"fieldList": [
									"childItems"
								],
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "BinaryReadSettings"
								}
							}
						},
						{
							"name": "WaitFalse",
							"type": "Wait",
							"dependsOn": [
								{
									"activity": "IfAnyFiles",
									"dependencyConditions": [
										"Failed"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"waitTimeInSeconds": 1
							}
						}
					]
				}
			}
		],
		"annotations": []
	}
}